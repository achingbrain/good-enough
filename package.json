{
  "name": "good-enough",
  "version": "4.2.0",
  "description": "A good logger with useful output",
  "scripts": {
    "test": "ava --verbose",
    "test:coverage": "nyc --all npm test && nyc report --reporter=html",
    "test:coverage:check": "npm run test:coverage && nyc check-coverage --lines 100",
    "lint": "standard",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Alex Potsides",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:achingbrain/good-enough.git"
  },
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "chalk": "^1.0.0",
    "moment": "^2.11.2",
    "prettysize": "0.0.3"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "chai": "^3.0.0",
    "coveralls": "^2.11.2",
    "faker": "^3.0.1",
    "istanbul": "^0.4.0",
    "nyc": "^7.1.0",
    "pre-commit": "^1.0.10",
    "proxyquire": "^1.5.0",
    "sinon": "^1.15.3",
    "standard": "^7.1.2",
    "through2": "^2.0.1"
  },
  "pre-commit": [
    "lint",
    "test:coverage:check"
  ],
  "nyc": {
    "exclude": [
      "coverage",
      "test"
    ]
  }
}
