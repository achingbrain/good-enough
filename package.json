{
  "name": "good-enough",
  "version": "0.0.2",
  "description": "A good logger with useful output",
  "scripts": {
    "test": "mocha --reporter=nyan",
    "test:coverage": "istanbul --include-all-sources cover _mocha",
    "test:coverage:check": "istanbul --include-all-sources cover _mocha && istanbul check-coverage --statement 100 --branch 100 --function 100",
    "lint": "standard",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "author": "Alex Potsides",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:achingbrain/good-enough.git"
  },
  "dependencies": {
    "chalk": "^1.0.0",
    "good-squeeze": "^2.1.0",
    "moment": "^2.10.3",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.4.0",
    "mocha": "^2.2.5",
    "pre-commit": "^1.0.10",
    "proxyquire": "^1.5.0",
    "sinon": "^1.15.3",
    "standard": "^5.3.1"
  },
  "pre-commit": [
    "lint",
    "test:coverage:check"
  ]
}
